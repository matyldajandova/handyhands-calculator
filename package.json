{
  "name": "handyhands-calculator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--no-deprecation' next dev --port 3000 --turbopack",
    "dev:smart": "./scripts/dev.sh",
    "dev:kill": "pkill -f 'next dev'",
    "prebuild": "npm run pdf:css",
    "build": "NODE_OPTIONS='--no-deprecation' next build",
    "start": "NODE_OPTIONS='--no-deprecation' next start",
    "lint": "next lint",
    "pdf:css": "tailwindcss -i src/pdf/input.css -o dist/pdf.css --minify --content 'src/pdf/**/*.{ts,tsx,html}'",
    "pdf:css:watch": "tailwindcss -i src/pdf/input.css -o dist/pdf.css --minify --watch --content 'src/pdf/**/*.{ts,tsx,html}'"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@next/third-parties": "^15.4.6",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sparticuz/chromium": "^138.0.2",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "googleapis": "^126.0.1",
    "lucide-react": "^0.539.0",
    "lz-string": "^1.5.0",
    "next": "15.4.6",
    "puppeteer-core": "^24.22.0",
    "react": "19.1.0",
    "react-day-picker": "^9.11.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-resizable-panels": "^3.0.4",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/cli": "^4.1.13",
    "@tailwindcss/postcss": "^4",
    "@types/lz-string": "^1.5.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "playwright": "^1.55.0",
    "puppeteer": "^24.21.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5"
  }
}
